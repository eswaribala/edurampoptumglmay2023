type Account{
    accountId: ID!
    runningTotal: Int
    openDate: String
    transactions: [Transaction]

}

type Query{
    showAccounts:[Account]
    showAccount(accountNo: ID!):Account
}

input AccountInput{
    accountId:ID!
    runningTotal:Int
    openDate: String
    transactions:[TransactionInput]
}
type Mutation{
    addAccount(accountInput:AccountInput!):Account
}
